{
  "network": {
    "title": "Network configuration defaults",
    "device_nr": "auto",
    "network_name": "cloudnet",
    "publish": "cloudnet",
    "password": "auto",

    "connect": [{
        "transport": "tls",
        "parameters": ":6367",
        "listen": 1,
        "flags": "listen,down"
      },
      {
        "transport": "none",
        "parameters": ":6369",
        "flags": "connect,down"
      }
    ],

    "security": [{
      "certchainfile": "myhome-bundle.crt",
      "certfile": "rootca.crt",
      "keyfile": "secret/rootca.key"
    }]
  }
}