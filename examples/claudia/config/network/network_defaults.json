{
  "network": {
    "title": "Network configuration defaults",
    "device_nr": "*",
    "network_name": "cloudnet",
    "publish": "cloudnet",
    "password": "",

    "connect": [{
        "transport": "none",
        "parameters": ":6366",
        "flags": "listen,up"
      },
      {
        "transport": "tls",
        "parameters": ":6367",
        "flags": "listen,down"
      }
    ],

    "security": [{
        "certfile": "myhome.crt",
        "keyfile": "secret/myhome.key",
        "trustcertfile": "rootca.crt"
    }]
  }
}
