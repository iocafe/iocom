        "pins": [
          {"name": "led_morse", "addr": 15},
          {"name": "led_builtin", "addr": 33}
        ]
      },
      {
        "name": "analog_inputs",
        "pins": [
          {"name": "ambient", "addr": 13, "max": 4095},
          {"name": "unused_pin", "addr": 14, "max": 4095}
        ]
      },
      {
        "name": "pwm",
        "pins": [
          {"name": "headlight", "bank": 1, "addr": 4, "frequency": 5000, "resolution": 12, "init": 0, "max": 4095}
