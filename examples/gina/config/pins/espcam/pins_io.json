            {"name": "DEV_SCK", "addr": 6},
            {"name": "DEV_SDO", "addr": 7},
            {"name": "DEV_SDI", "addr": 8},
            {"name": "DEV_SHD", "addr": 9},
            {"name": "DEV_SWP", "addr": 10},
            {"name": "DEV_CSC", "addr": 11}
          ]
      }
    ]
  }]
}
