{
  "io": [{
    "name": "gina",
    "title": "IO pin setup for the 'gina' example application on 'espcam' hardware",
    "groups": [
      {
        "name": "inputs",
        "pins": [
          {"name": "gazerbeam", "addr": 12, "interrupt": 1}
        ]
      },
      {
