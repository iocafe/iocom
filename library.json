{
  "name": "iocom",
  "description": "iocom protocol",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/iocafe/iocom.git"
  },
  "frameworks": [
     "arduino", 
     "espidf"
  ],
  "platforms": [
    "teensy", 
    "stm32",
    "espressif32"
  ],
  
  "build": {
    "srcFilter": [
      "+<code/*>",
      "+<extensions/dynamicio/*>"
    ],
    "libLDFMode" : "chain"
  },
  
  "dependencies": [
    {
        "name": "eosal"
    }  
  ]
}
